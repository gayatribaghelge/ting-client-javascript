{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "module": "system",
    "moduleResolution": "node",
    "target": "es5",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "compiled"
  }
}